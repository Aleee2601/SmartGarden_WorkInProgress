# SmartGarden React App Environment Variables

# API Configuration
REACT_APP_API_URL=https://localhost:5000/api

# Development Mode
NODE_ENV=development

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_NOTIFICATIONS=true

# Sensor Configuration
REACT_APP_SENSOR_POLL_INTERVAL=60000
REACT_APP_CALIBRATION_COUNTDOWN=10
